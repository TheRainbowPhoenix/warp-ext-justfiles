{
    'name' 'JSTORE'
    'desc' 
    <'
Stored data in a Just file.
    '>
    'sig' [
      [ [ 'pairs:LIST<LIST>' 'uri:STRING' ] [] ]
    ]
    'params' {
      'pairs' 'List of `[ key value ]` pairs, `key` being a STRING and `value` either a STRING or byte array. STRING values will be converted to byte arrays using UTF-8.'
      'uri' 'JFile URI'
    }
    'examples' [
      <'
'test.jfile'
[
  [ 'foo' 'foo:value' ]
  [ 'bar' 'bar:value' ]
]
JSTORE
      '>
    ]
    'related' [ 'JLOAD' ]
    'ext' 'io.warp1.script.ext.jfiles.JFilesWarpScriptExtension'
} 'info' STORE

<%
  !$info INFO
  SAVE 'context' STORE
    <% 'JSTORE' EVAL %> <% %> <% %> TRY    
  $context RESTORE
%>
'macro' STORE
$macro
